{
	"name": "coliving",
	"author": {
		"name": "Sarvex Jatasra",
		"email": "sarvex@jatasra.com",
		"url": "https://jatasra.com"
	},
	"homepage": "https://coliving.ai",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@mui/material": "5.14.6",
		"@mui/icons-material": "5.14.6",
		"@types/googlemaps": "3.43.3",
		"@types/jest": "29.5.4",
		"@types/markerclustererplus": "2.1.33",
		"@types/node": "20.5.6",
		"@types/prop-types": "15.7.5",
		"@types/react": "18.2.20",
		"@types/react-dom": "18.2.7",
		"animate.css": "4.1.1",
		"apollo-boost": "0.4.9",
		"classnames": "2.3.2",
		"enzyme": "3.11.0",
		"eslint-import-resolver-babel-module": "5.3.2",
		"firebase": "10.2.0",
		"graphql": "16.8.0",
		"graphql-tag": "2.12.6",
		"history": "5.3.0",
		"mobx": "6.10.0",
		"moment": "2.29.4",
		"node-sass": "9.0.0",
		"nouislider": "15.7.1",
		"prop-types": "15.8.1",
		"react": "18.2.0",
		"react-animate-on-scroll": "2.1.7",
		"react-apollo": "3.1.5",
		"react-datetime": "3.2.0",
		"react-dom": "18.2.0",
		"react-google-maps": "9.4.5",
		"react-image-gallery": "1.3.0",
		"react-intl": "6.4.4",
		"react-router": "6.15.0",
		"react-router-dom": "6.15.0",
		"react-router-sitemap": "1.2.0",
		"react-scripts": "5.0.1",
		"react-slick": "0.29.0",
		"react-swipeable-views": "0.14.0",
		"react-tagsinput": "3.20.3",
		"typescript": "5.1.6"
	},
	"devDependencies": {
		"@babel/cli": "7.22.10",
		"@babel/core": "7.22.11",
		"@babel/preset-env": "7.22.10",
		"@babel/preset-react": "7.22.5",
		"babel-loader": "9.1.3",
		"core-js": "3.32.1",
		"eslint": "8.47.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-config-prettier": "9.0.0",
		"eslint-loader": "4.0.2",
		"eslint-plugin-import": "2.28.1",
		"eslint-plugin-prettier": "5.0.0",
		"eslint-plugin-promise": "6.1.1",
		"eslint-plugin-react": "7.33.2",
		"prettier": "3.0.2",
		"prettier-eslint": "15.0.1",
		"prettier-eslint-cli": "7.1.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"lint": "eslint . --ext=js,jsx --fix; exit 0",
		"fix-code": "prettier-eslint --write 'src/**/*.{js,jsx}' ",
		"fix-styles": "prettier-stylelint --write 'src/**/*.{css,scss}' ",
		"clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start"
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-react"
		]
	},
	"eslintConfig": {
		"root": true,
		"parser": "babel-eslint",
		"env": {
			"browser": true,
			"node": true,
			"es6": true
		},
		"globals": {
			"use": true
		},
		"parserOptions": {
			"ecmaVersion": 6,
			"sourceType": "module",
			"ecmaFeatures": {
				"jsx": true
			}
		},
		"extends": [
			"airbnb",
			"prettier",
			"prettier/react",
			"react-app"
		],
		"plugins": [
			"prettier",
			"react"
		],
		"rules": {
			"implicit-arrow-linebreak": "off",
			"comma-dangle": "off",
			"indent": "off",
			"no-trailing-spaces": "off",
			"no-param-reassign": "off"
		},
		"settings": {
			"import/resolver": {
				"node": {
					"paths": [
						"src"
					]
				}
			}
		}
	},
	"prettier": {
		"tabWidth": 2,
		"printWidth": 120,
		"singleQuote": true,
		"jsxSingleQuote": true,
		"semi": false,
		"useTabs": false,
		"trailingComma": "es5",
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"arrowParens": "always",
		"endOfLine": "cr"
	},
	"engines": {
		"node": "18.x",
		"npm": "9.x"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
